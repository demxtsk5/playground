$TTL    604800
; SOA record with MNAME and RNAME updated
@       IN      SOA     {{domain_name}}. root.{{domain_name}}. (
                              2         ; Serial Note: increment after each change
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
; Name server record 
@       IN      NS      u01.{{domain_name}}.
; A record for name server
u01      IN      A       192.168.65.17
; PTR record for clients
27   IN      PTR     k8s-m1.{{domain_name}}
28   IN      PTR     k8s-n1.{{domain_name}}
29   IN      PTR     k8s-n2.{{domain_name}}
